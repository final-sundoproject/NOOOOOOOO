<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_background_rouded"
    android:paddingTop="1dp">

    <ImageView
        android:id="@+id/menuButton"
        android:layout_width="29dp"
        android:layout_height="21dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:elevation="10dp"
        android:src="@drawable/menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/logoImage"
        android:layout_width="410dp"
        android:layout_height="730dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="10dp"
        android:scaleType="fitXY"
        android:src="@drawable/samplemap" />

    <TextView
        android:id="@+id/textBox3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/text_box_background"
        android:padding="5dp"
        android:text="이대역"
        android:textAlignment="center"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/arCheck" />

    <Button
        android:id="@+id/arCheck"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="AR 확인"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/coordinateInput"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteY="57dp" />

    <Button
        android:id="@+id/coordinateInput"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="좌표입력"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="57dp" />

    <Button
        android:id="@+id/coordinateSelect"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="좌표선택"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/coordinateInput"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteY="57dp" />


    <Button
        android:id="@+id/back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="#"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="96dp" />


    <Button
        android:id="@+id/gps"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="#"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="58dp" />

    <Button
        android:id="@+id/remove"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="#"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteX="366dp"
        tools:layout_editor_absoluteY="96dp" />


    <Button
        android:id="@+id/pointList"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="점수목록"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteX="295dp"
        tools:layout_editor_absoluteY="627dp" />

    <Button
        android:id="@+id/enteredPoint"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/login_button_background"
        android:paddingTop="3dp"
        android:text="점수입력"
        android:textAlignment="center"
        android:textColor="#ffff"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintWidth_percent="0.3"
        tools:layout_editor_absoluteX="295dp"
        tools:layout_editor_absoluteY="665dp" />


</androidx.constraintlayout.widget.ConstraintLayout>