<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">



    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sundo_project_app"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".project.AddbusinessActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.ACTION_ADD_BUSINESS"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".login.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.LOGIN_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>


        <activity android:name=".location.MapActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".location.ChoiceCooridate"
            android:theme="@style/CustomDmsTheme"
            android:exported="true">
            <!-- intent-filter 제거 -->
        </activity>

        <activity
            android:name=".location.DmsActivity"
            android:theme="@style/CustomDmsTheme"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.DMS_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".login.SignUpActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.SIGNUP_ACTION" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".login.EmailFindActivity" />

        <activity

            android:name=".location.DdActivity"
            android:theme="@style/CustomDmsTheme"
            android:exported="true">
            <!-- intent-filter 제거 -->
        </activity>

        <activity
            android:name=".location.GeneratorActivity"
            android:theme="@style/CustomGeneratorTheme"
            android:exported="true">
            <!-- intent-filter 제거 -->
        </activity>

        <activity
            android:name=".evaluation.EvaluationActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.EVALUATION_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".evaluation.EvaluationFindAllActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.EVALUATIONFINDALL_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="nbpr5wd89w" />

    </application>

</manifest>
